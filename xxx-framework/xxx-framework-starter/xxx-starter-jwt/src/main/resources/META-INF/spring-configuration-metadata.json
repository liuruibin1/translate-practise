{
  "groups": [
    {
      "name": "framework.jwt",
      "type": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties"
    }
  ],
  "properties": [
    {
      "name": "framework.jwt.secret",
      "type": "java.lang.String",
      "description": "秘钥",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties"
    },
    {
      "name": "framework.jwt.issuer",
      "type": "java.lang.String",
      "description": "发行人；默认com.xxx",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "defaultValue": "com.xxx"
    },
    {
      "name": "framework.jwt.user-id-claim-key",
      "type": "java.lang.String",
      "description": "用户ID声明key,默认: user_id",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "defaultValue": "user_id"
    },
    {
      "name": "framework.jwt.visitor-id-claim-key",
      "type": "java.lang.String",
      "description": "访客ID声明key,默认: visitor_id",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "defaultValue": "visitor_id"
    },
    {
      "name": "framework.jwt.token-valid-duration",
      "type": "java.lang.Long",
      "description": "Token过期时间，单位：分钟；默认1440分钟",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "defaultValue": 1440
    },
    {
      "name": "framework.jwt.token-refresh-expire",
      "type": "java.lang.Long",
      "description": "刷新Token过期时间，单位：分钟；默认720分钟",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "defaultValue": 720
    },
    {
      "name": "framework.jwt.refresh-token-format",
      "type": "java.lang.String",
      "description": "刷新Token存储Key格式；默认 JWT_REFRESH_TOKEN::%s",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "defaultValue": "JWT_REFRESH_TOKEN::%s"
    },
    {
      "name": "framework.jwt.blacklist-format",
      "type": "java.lang.String",
      "description": "黑名单存储Key格式；默认 JWT_BLACKLIST::%s",
      "sourceType": "com.xxx.framework.jwt.configuration.properties.JWTProperties",
      "defaultValue": "JWT_BLACKLIST::%s"
    }
  ]
}





